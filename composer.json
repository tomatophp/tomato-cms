{
    "name": "tomatophp/tomato-cms",
    "type": "library",
    "description": "full CMS System to manage your content build for Tomato PHP",
    "keywords": [
        "php",
        "laravel",
        "template"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoCms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoCms\\TomatoCmsServiceProvider"
            ],
            "aliases": {
                "TomatoCMS": "TomatoPHP\\TomatoCms\\Facades\\TomatoCMS"
            }
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "tomatophp/tomato-admin": "^1.2",
        "tomatophp/tomato-crm": "^1.1",
        "tomatophp/tomato-forms": "^1.1",
        "tomatophp/tomato-category": "^1.1",
        "laravel/dusk": "^8.2"
    }
}
